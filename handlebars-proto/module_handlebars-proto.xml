      <bootclasspath refid="handlebars-proto.module.bootclasspath"/>
      <classpath refid="handlebars-proto.module.production.classpath"/>
      <src refid="handlebars-proto.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.handlebars-proto"/>
    </javac2>
    
    <copy todir="${handlebars-proto.output.dir}">
